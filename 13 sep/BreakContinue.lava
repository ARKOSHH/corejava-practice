/*
Program: Break and Continue
*/

class BreakContinue
{
  static void printNumbers()
  {
	  for(int i=1; i<=10 && i!=5 ;i++)
	  {
	  
		  System.out.println(i);
		  
	  }
	  

  public static void main(String ... args)
  {
	  System.out.println("Printing numbers without 5");
	  printNumbers();
  }
  
}